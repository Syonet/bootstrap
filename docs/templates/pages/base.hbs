    <!-- Tipografia -->
    <article id="type">
        <div class="syo-header">
            <h2>Tipografia</h2>
        </div>

        <h3>Headings</h3>
        <p>Todos os headings HTML, do <code>h1</code> ao <code>h6</code>.</p>
        <div class="syo-docs-example">
            <div class="syo-docs-example-inner">
                <h1>h1. Heading 1</h1>
                <h2>h2. Heading 2</h2>
